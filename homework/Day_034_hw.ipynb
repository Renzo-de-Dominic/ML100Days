   "source": [
    "import statsmodels.stats.proportion\n",
    "A = [75,30] # 有開信的個數\n",
    "B = [300,300] #各組實驗總個數\n",
    "C=statsmodels.stats.proportion.proportions_ztest(A, B, alternative='two-sided')\n",
    "if C[1]<0.05:\n",
    "    print(\"產品不良率不相同\")\n",
    "else:\n",
    "    print(\"產品不良率不一定不相同\")"
   ]
